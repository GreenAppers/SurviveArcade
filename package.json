{
  "name": "SurvivePinball",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn build-ts && yarn build-place",
    "build-place": "rojo build -o SurvivePinball.rbxlx",
    "sync": "rojo serve",
    "syncback": "rojo-syncback syncback --input ./SurvivePinball.rbxlx 2>&1 | tee syncback.log && tools/rbxm2rbxmx.sh",
    "build-ts": "rbxtsc",
    "watch-ts": "rbxtsc -w"
  },
  "devDependencies": {
    "@rbxts/compiler-types": "^2.2.0-types.0",
    "@rbxts/types": "^1.0.736",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.0",
    "rbxts-transformer-flamework": "^1.0.0-beta.24",
    "roblox-ts": "^2.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@flamework/components": "^1.0.0-beta.24",
    "@flamework/core": "^1.0.0-beta.24",
    "@flamework/networking": "^1.0.0-beta.24",
    "@rbxts/profileservice": "^1.4.2",
    "@rbxts/react-redux": "^0.1.0",
    "@rbxts/react-roblox": "^0.2.0",
    "@rbxts/ripple": "^0.6.0",
    "@rbxts/roact": "npm:@rbxts/react-ts",
    "@rbxts/services": "^1.5.1"
  }
