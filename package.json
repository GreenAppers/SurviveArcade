{
  "name": "SurviveArcade",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn compile && yarn build-place",
    "build-place": "rojo build -o SurviveArcade.rbxlx",
    "compile": "rbxtsc",
    "lint": "yarn eslint src && yarn prettier --check src",
    "sync": "rojo serve",
    "syncback": "rojo-syncback syncback --input ./SurviveArcade.rbxlx 2>&1 | tee syncback.log && tools/rbxm2rbxmx.sh && rm -rf node_modules && yarn",
    "tarmac": "tarmac sync --target roblox",
    "watch": "rbxtsc -w"
  },
  "devDependencies": {
    "@rbxts/cmdr": "^1.9.0-ts.0",
    "@rbxts/compiler-types": "^2.2.0-types.0",
    "@rbxts/types": "^1.0.736",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.1.0",
    "rbxts-transform-debug": "^2.2.0",
    "rbxts-transform-env": "^2.2.0",
    "rbxts-transformer-flamework": "^1.0.0-beta.24",
    "roblox-ts": "^2.2.0",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@flamework/components": "^1.0.0-beta.24",
    "@flamework/core": "^1.0.0-beta.24",
    "@flamework/networking": "^1.0.0-beta.24",
    "@rbxts/flipper": "^2.0.1",
    "@rbxts/object-utils": "^1.0.4",
    "@rbxts/pretty-react-hooks": "^0.3.3",
    "@rbxts/profileservice": "^1.4.2",
    "@rbxts/react-reflex": "^0.2.0",
    "@rbxts/react-roblox": "^0.2.0",
    "@rbxts/reflex": "^4.3.1",
    "@rbxts/ripple": "^0.6.0",
    "@rbxts/roact": "npm:@rbxts/react-ts",
    "@rbxts/services": "^1.5.1"
  }
}
