{
  "name": "SurvivePinball",
  "globIgnorePaths": [
    "**/package.json",
    "**/tsconfig.json"
  ],
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$path": "src/ReplicatedStorage"
    },
    "ServerScriptService": {
      "$path": "src/ServerScriptService"
    },
    "StarterPlayer": {
      "StarterCharacterScripts": {
        "$path": "src/StarterPlayer/StarterCharacterScripts"
      },
      "StarterPlayerScripts": {
        "$path": "src/StarterPlayer/StarterPlayerScripts"
      }
    },
    "Teams": {
      "$path": "src/Teams"
    },
    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "$path": "src/Workspace"
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0.0,
          0.0,
          0.0
        ],
        "Brightness": 2.0,
        "ClockTime": 23.5,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "ShadowMap"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
